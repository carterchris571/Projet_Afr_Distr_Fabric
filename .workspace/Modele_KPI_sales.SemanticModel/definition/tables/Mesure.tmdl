	measure 'Nett adds' =
			
			VAR DerniereDateDisponible =
			    CALCULATE(
			        MAX(Net_adds_cluster[Date]),
			        ALLSELECTED(Net_adds_cluster)
			    )
			RETURN
			CALCULATE(
			    SUM(Net_adds_cluster[Net_adds]),
			    FILTER(
			        ALL(Net_adds_cluster[Date]),
			        Net_adds_cluster[Date] = DerniereDateDisponible
			    )
			)
		formatString: 0
		lineageTag: 2ecea213-3e4b-45dc-8545-ea46b09c694c

	measure Taux_couv = ```
			
			VAR Points_Touches = 
			    CALCULATE(
			        DISTINCTCOUNT(Segmentation[Num_POS]),
			        FILTER(
			             Segmentation,
			            CALCULATE(
			                COUNTROWS(Listing_union_nettoye),
			                Listing_union_nettoye[Amount] < 0, 
			                Listing_union_nettoye[Type] = "Transfer",
			                ABS(Listing_union_nettoye[Amount]) >= 1000,
			                FILTER(
			                    Listing_union_nettoye,
			                    Listing_union_nettoye[To_num] = Segmentation[Num_POS]
			                 )
			            ) > 0
			        )
			    )
			
			VAR Total_POS = DISTINCTCOUNT(Segmentation[Num_POS])
			
			RETURN 
			DIVIDE(Points_Touches,Total_POS,0)
			```
